{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.html", "/*.js", "/*.css", "/*.json"]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
